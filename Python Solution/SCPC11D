while True:
    arr = map(int , raw_input().split())
    arr = sorted(arr)
    a , b , c = arr[0] , arr[1] , arr[2]
    if a + b + c == 0:
        break
    else:
        
        if a + b >= c:
            if a**2 + b**2 == c**2:
                print "right"
            else:
                print "wrong"
        else:
            print "wrong"
